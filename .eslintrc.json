{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "error",
      "react/jsx-uses-react": "off",
      "react/jsx-uses-vars": "error"
    }
  }